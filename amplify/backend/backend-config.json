{
	"auth": {
		"awsamplifyecommerceauth": {
			"service": "Cognito",
			"providerPlugin": "awscloudformation",
			"dependsOn": []
		}
	},
	"analytics": {
		"awsamplifyecommerce": {
			"service": "Pinpoint",
			"providerPlugin": "awscloudformation"
		}
	},
	"storage": {
		"dynamodbitems": {
			"service": "DynamoDB",
			"providerPlugin": "awscloudformation"
		}
	},
	"function": {
		"lambdaitems": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "storage",
					"resourceName": "dynamodbitems",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	},
	"api": {
		"awsamplifyecommerceapi": {
			"service": "API Gateway",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "lambdaitems",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	},
	"hosting": {
		"S3AndCloudFront": {
			"service": "S3AndCloudFront",
			"providerPlugin": "awscloudformation"
		}
	}
}